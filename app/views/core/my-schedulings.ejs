<!DOCTYPE html>
<html lang="en">

<head>

  <%-include('./include/head-links.ejs')%>

  <link href="" rel="stylesheet">
  
  <title>Bem-Vindo!</title>

<link href="core/main.550dcf66.css" rel="stylesheet"></head>

<body> <!-- Add your content of header -->
<header>
  
</header>

<!-- Add your site or app content here -->
  <div class="background-image-container white-text-container">
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          <h1>Meus Agendamentos</h1>                    
        </div>
      </div>
    </div>
  </div>

  <div class="section-container">
    <div class="container text-center">

    <% if(typeof message != 'undefined' && message != '') { %>
    <div class="col-sm-12">
      <div class="alert  alert-success alert-dismissible fade show" role="alert">
        <span class="badge badge-pill badge-success">Success</span> <%=message%>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
    </div>
    <% } %>
      
    <div class="panel">
        <% if(typeof mySchedulings != 'undefined' 
          && Object.keys(mySchedulings).length > 0) { %>

            <% for(scheduling of mySchedulings) { %>
            <form method="post" action="/core-cancel-scheduling">
				<input type="hidden" name="websiteUser" value="<%=scheduling.website%>">                
                <input type="hidden" name="schedulingId" value="<%=scheduling.schedulingId%>">
                <div class="form-group row">
                    <div class="col-sm-6">
                        <input type="text" class="form-control form-control-sm" 
                            style="text-align: center"
                            value="Agendado com <%=scheduling.employeeName%>" readonly>
                    </div>
                    <div class="col-sm-4">
                        <input type="text" class="form-control form-control-sm"
                            style="text-align: center"
                            value="<%=scheduling._datetime.toLocaleString()%>" readonly>
                    </div>
                    <div class="col-sm-2">
                        <input type="submit" class="form-control form-control-sm btn btn-danger" value="Cancelar"/>
                    </div>
                </div>            
            </form>
            <hr>
            <% } %>

        <% } else { %>
            <div class="alert alert-warning">
                <strong>Aviso!</strong> Você não tem nenhum agendamento.                     
                <a href="#" class="alert-link" onClick="javascript:history.go(-1)">Voltar</a>.
            </div>
        <% } %>

    </div>




    </div>
  </div>
  </div>

  <%-include('./include/contact-container.ejs')%>

<script>
  document.addEventListener("DOMContentLoaded", function (event) {
    navbarFixedTopAnimation();
  });
</script>

<%-include('./include/footer.ejs')%>

