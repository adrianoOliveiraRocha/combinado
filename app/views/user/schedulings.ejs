<html class="no-js" lang="pt-BR">
<!--<![endif]-->

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Bem Vindo!</title>
  <meta name="description" content="Combinado">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <%-include('./include/top-links-table.ejs')%>

</head>

<body onload="callNotifications()">


  <!-- Left Panel -->

  <%-include('./include/left-panel.ejs')%>

  <!-- Left Panel -->

  <!-- Right Panel -->

  <div id="right-panel" class="right-panel">

    <!-- Header-->
    <%-include('./include/header.ejs')%>
    <!-- Header-->
    <div class="content mt-3" id="divResponse">
      <div class="animated fadeIn">
        <div class="row">
          <div class="col-md-12">
            <div class="card">
              <div class="card-header">
                <strong class="card-title">Agendamentos / <%=status%></strong>
              </div>
              <div class="card-body">
                <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                  <thead>
                    <tr>
                      <th>Data</th>
                      <th>Colaborador</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(let scheduling of schedulings) {  %>
                    <tr>
                      <td>
                        <a href="#" onclick="Scheduling.showScheduling('<%=scheduling.schedulingId%>')">
                          <% if(isFuture(scheduling._datetime)) { %>
                          <div class="alert alert-primary" style="text-align: center">
                            <%=portugueseDateTime(scheduling._datetime)%>
                          </div>
                          <% } else { %>
                          <div class="alert alert-secondary" style="text-align: center">
                            <%=portugueseDateTime(scheduling._datetime)%>
                          </div>
                          <% } %>
                        </a>
                      </td>
                      <td>
                        <a href="#" onclick="Scheduling.showScheduling('<%=scheduling.schedulingId%>')"><%=scheduling.name%></a>
                      </td>
                    </tr>
                    <% } %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div><!-- .animated -->

    </div> <!-- .content -->
  </div><!-- /#right-panel -->
  <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
  <%-include('./include/bottom-links-table.ejs')%>


</body>

</html>
