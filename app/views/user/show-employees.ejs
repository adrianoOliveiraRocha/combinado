<div class="animated fadeIn">
	<div class="row">

		<div class="col-md-12">
			<div class="card">
				<div class="card-header">
						<strong class="card-title">Todos os Colaboradores</strong>
				</div>
				<div class="card-body">
					<table id="bootstrap-data-table-export" class="table table-striped table-bordered">
						<thead>
							<tr>
								<th>Name</th>
								<th>Email</th>

							</tr>
						</thead>
						<tbody>
							<% for(employee of employees) { %>
							<!-- user data -->
							<input type="hidden" id="email_<%=employee.id%>" value="<%=employee.email%>">
							<input type="hidden" id="pwd_<%=employee.id%>" value="<%=employee.pwd%>">
							<input type="hidden" id="name_<%=employee.id%>" value="<%=employee.name%>">
							<input type="hidden" id="phone_<%=employee.id%>" value="<%=employee.phone%>">
							<input type="hidden" id="address_<%=employee.id%>" value="<%=employee.address%>">
							<input type="hidden" id="userid_<%=employee.id%>" value="<%=employee.userid%>">
							<input type="hidden" id="aboutMe_<%=employee.id%>" value="<%=employee.aboutMe%>">
							<input type="hidden" id="image_<%=employee.id%>" value="<%=employee.image%>">
							<input type="hidden" id="type_<%=employee.id%>" value="<%=employee.type%>">
							<!-- end user data -->
							<tr>
								<td>
									<a href="#" onclick="employeeDetail('<%=employee.id%>')">
										<%=employee.name%>
									</a>
								</td>
								<td>
									<a href="#" onclick="employeeDetail('<%=employee.id%>')">
										<%=employee.email%>
									</a>
								</td>
							</tr>
							<% } %>
						</tbody>
					</table>

				</div>
			</div>
		</div>


	</div>
</div><!-- .animated -->
